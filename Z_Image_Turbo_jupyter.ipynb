        "#@title # **Run ComfyUI GUI** ðŸš€\n",
        "# All necessary imports goes here\n",
        "\n",
        "import subprocess\n",
        "import threading\n",
        "import time\n",
        "import socket\n",
        "import sys\n",
        "\n",
        "SERVER_PORT = 8188\n",
        "\n",
        "def tunnel_thread(port):\n",
        "\n",
        "    while True:\n",
        "        try:\n",
        "            sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
        "            # Set a small timeout for the connection attempt\n",
        "            sock.settimeout(1)\n",
        "            result = sock.connect_ex(('127.0.0.1', port))\n",
        "            sock.close()\n",
        "            if result == 0:\n",
        "                break\n",
        "        except Exception:\n",
        "            # Handle potential socket errors during checks\n",
        "            pass\n",
        "        time.sleep(1) # Check once per second\n",
        "\n",
        "\n",
        "    tunnel_command = [\"cld\", \"tunnel\", \"--url\", \"http://127.0.0.1:{}\".format(port)]\n",
        "\n",
        "    p = subprocess.Popen(\n",
        "        tunnel_command,\n",
        "        stdout=subprocess.PIPE,\n",
        "        stderr=subprocess.STDOUT, # Direct stderr to stdout for easier reading\n",
        "        universal_newlines=True # Use text mode for reading lines\n",
        "    )\n",
        "\n",
        "    for line in p.stdout:\n",
        "        # Check for the public URL\n",
        "        if \"trycloudflare.com \" in line:\n",
        "            public_url = line[line.find(\"http\"):].strip()\n",
        "            print(\"==================================================\")\n",
        "            print(\"ðŸš€ **PUBLIC ACCESS URL:**\", public_url)\n",
        "            print(\"==================================================\")\n",
        "\n",
        "\n",
        "threading.Thread(target=tunnel_thread, daemon=True, args=(SERVER_PORT,)).start()\n",
        "\n",
        "!python main.py"
      ],
